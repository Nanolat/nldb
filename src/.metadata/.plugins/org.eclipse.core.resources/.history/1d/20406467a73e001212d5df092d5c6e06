#ifndef NLDBTREE_H_
#define NLDBTREE_H_



template<int key_size, int value_size> class nldb_tree {
	class key_value {
		char key_  [key_size];
		char value_[value_size];
	};

	class node {
		char min_key_  [key_size];
		node * parent_;
		node * next_;
	};

public:
	nldb_tree();
	virtual ~nldb_tree();


};

#endif /* NLDBTREE_H_ */
